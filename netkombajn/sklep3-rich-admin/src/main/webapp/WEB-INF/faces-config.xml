        </managed-property>
		<managed-property>
            <property-name>configuration</property-name>
            <value>#{configuration}</value>
        </managed-property>
		<managed-property>
            <property-name>globalImportBackingBean</property-name>
            <value>#{globalImportBacking}</value>
