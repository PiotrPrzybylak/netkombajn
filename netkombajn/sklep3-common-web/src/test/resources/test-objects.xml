<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean class="org.springframework.web.servlet.mvc.SimpleControllerHandlerAdapter"/>
	<bean id="productDao" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="pl.netolution.sklep3.dao.ProductDao" />
	</bean>
	<bean id="productRatingDao" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="pl.netolution.sklep3.dao.hibernate.ProductRatingDao" />
	</bean>
	
</beans>
